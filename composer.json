    "description": "Symfony Datatable Bundle For Doctrine2 Entities",
    "keywords": [
        "symfony3",
        "symfony4",
        "symfony",
        "datatable",
        "datatables",
        "data-table",
        "encore",
        "grid",
        "table",
        "datagrid",
        "pagination",
        "webpack"
    ],
    "homepage": "https://github.com/debricked/DatatablesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "stwe"
        },
        {
            "name": "sweoggy"
        },
        {
            "name": "debricked AB",
            "homepage": "https://debricked.com"
        },
        {
            "name": "SgDatatablesBundle contributors",
            "homepage": "https://github.com/sweoggy/DatatablesBundle/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/sweoggy/DatatablesBundle/issues"
