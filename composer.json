        "symfony/translation": "^4.4|^5.1",
        "symfony/twig-bundle": "^4.4|^5.1",
        "twig/twig": "^2.0.0|^3.0.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.3",
        "symfony/asset": "^4.4|^5.1",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Sg\\DatatablesBundle\\": ""
        }
    }
